                <section class="space--xxs">
                    <div class="container">
                        <div class="row text-center">
                            <div class='col-sm-12'>
                                <h3 class="banner" data-item="title_1">
                                    <%= __('title').wormhole_protocol %>
                                </h3>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12">
                                <p data-item="title_1_1">
                                    <%= __('title').wormhole_protocol_desc %>
                                </p>

                            </div>
                        </div>
                    </div>
                </section>
                <div id="futures"></div>
                <section class="space--xxs">
                    <div class="container">
                        <div class="row text-center">
                            <div class='col-sm-12'>
                                <h3 data-item="title_2">
                                    <%= __('title').future_development %>
                                </h3>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 col-md-12">
                                <p>
                                    <strong data-item="title_2_1">
                                        <%= __('title').subTitle.permissionless_smart_contract %>
                                    </strong>
                                    <span data-item="title_2_2">
                                        <%= __('title').subTitle.permissionless_smart_contract_desc %>
                                    </span>
                                </p>
                                <p>
                                    <strong data-item="title_2_3">
                                        <%= __('title').subTitle.decentralized_exchange_protocol %>
