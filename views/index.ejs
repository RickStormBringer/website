                                    <strong data-item="feature_3">
                                        <%= __('feature').low_fees %>
                                    </strong>
                                </p>
                                <p data-item="feature_3_1">
                                    <%= __('feature').low_fees_desc %>
                                </p>
                            </div>
                        </div>
                        <div class="row text-center">
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-rss fa-fw fa-2x"></i>
                                </p>
                                <p>
                                    <strong data-item="feature_4">
                                        <%= __('feature').scalability %>
                                    </strong>
                                </p>
                                <p data-item="feature_4_1">
                                    <%= __('feature').scalability_desc %>
                                </p>
                            </div>
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-cubes fa-fw fa-2x"></i>
                                </p>
                                <p>
                                    <strong data-item="feature_5">
                                        <%= __('feature').decentralization %>
                                    </strong>
                                </p>
                                <p data-item="feature_5_1">
                                    <%= __('feature').decentralization_desc %>
                                </p>
                            </div>
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-lock fa-fw fa-2x"></i>
                                </p>
                                <p>
                                    <strong data-item="feature_6">
                                        <%= __('feature').security %>
                                    </strong>
                                </p>
                                <p data-item="feature_6_1">
                                    <%= __('feature').security_desc %>
                                </p>
                            </div>
                        </div>
                    </div>
                </section>
