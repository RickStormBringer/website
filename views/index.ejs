                <br>
                <section class="space--xxs no-bottom icon-container">
                    <div class="container benefits">
                        <div class="row text-center">
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-bolt fa-fw fa-2x"></i>
                                </p>
                                <p>
                                    <strong data-item="feature_1">
                                        <%= __('feature').high_speed %>
                                    </strong>
                                </p>
                                <p data-item="feature_1_1">
                                    <%= __('feature').high_speed_desc %>
                                </p>
                            </div>
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-tasks fa-fw fa-2x"></i>
                                </p>
                                <p>
                                    <strong data-item="feature_2">
                                        <%= __('feature').smart_contract %>
                                    </strong>
                                </p>
                                <p data-item="feature_2_1">
                                    <%= __('feature').smart_contract_desc %>
                                </p>
                            </div>
                            <div class="col-sm-4 col-md-4">
                                <p>
                                    <i class="fa fa-arrow-alt-down fa-fw fa-2x"></i>
                                </p>
